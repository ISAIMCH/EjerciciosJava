digraph Problema5_2 {
    rankdir=TB;
    node [fontname="Arial", fontsize=11, style=filled, fillcolor=white];
    edge [fontname="Arial", fontsize=10];

    // Nodos principales - Inicio
    inicio [shape=ellipse, label="Inicio", fillcolor="#4A90E2"];
    
    // Entrada de datos
    entrada_n [shape=box, label="LEER N\n(Número de empleados)", fillcolor="#B3E5FC"];
    entrada_empleados [shape=box, label="PARA I = 0 HASTA N-1:\n  LEER número[I]\n  LEER nombre[I]\n  LEER salario[I]\n  PARA J = 0 HASTA 11:\n    LEER ventas[I][J]", fillcolor="#B3E5FC"];
    
    // Inciso a) - Empleado con mayores ventas
    proc_a_init [shape=box, label="maxVentas = -1\nempleadoMax = -1", fillcolor="#FFE082"];
    proc_a_loop [shape=diamond, label="¿Para cada empleado I?", fillcolor="#FFD54F"];
    proc_a_suma [shape=box, label="suma = 0\nPARA J = 0 HASTA 11:\n  suma = suma + ventas[I][J]", fillcolor="#B3E5FC"];
    proc_a_comp [shape=diamond, label="¿suma >\nmaxVentas?", fillcolor="#FFD54F"];
    proc_a_upd [shape=box, label="maxVentas = suma\nempleadoMax = I", fillcolor="#B3E5FC"];
    salida_a [shape=egg, label="Salida A:\nNúmero, Nombre,\nTotal de ventas", fillcolor="#A5D6A7"];
    
    // Inciso b) - Incremento de salarios
    proc_b_loop [shape=diamond, label="¿Para cada empleado I?", fillcolor="#FFD54F"];
    proc_b_suma [shape=box, label="suma = 0\nPARA J = 0 HASTA 11:\n  suma = suma + ventas[I][J]", fillcolor="#B3E5FC"];
    proc_b_comp [shape=diamond, label="¿suma >\n1000000?", fillcolor="#FFD54F"];
    proc_b_upd [shape=box, label="salarioAnterior = salario[I]\nsalario[I] = salario[I] * 1.1", fillcolor="#B3E5FC"];
    salida_b [shape=egg, label="Salida B:\nNombre,\nSalario Anterior,\nNuevo Salario", fillcolor="#A5D6A7"];
    
    // Inciso c) - Empleados con ventas bajas en diciembre
    proc_c_loop [shape=diamond, label="¿Para cada empleado I?", fillcolor="#FFD54F"];
    proc_c_comp [shape=diamond, label="¿ventas[I][11]\n< 30000?", fillcolor="#FFD54F"];
    salida_c [shape=egg, label="Salida C:\nNúmero, Nombre,\nVentas Diciembre", fillcolor="#A5D6A7"];
    
    // Fin
    fin [shape=ellipse, label="Fin", fillcolor="#4A90E2"];

    // Conexiones
    inicio -> entrada_n;
    entrada_n -> entrada_empleados;
    entrada_empleados -> proc_a_init;
    
    // Inciso A
    proc_a_init -> proc_a_loop;
    proc_a_loop -> proc_a_suma [label="Sí"];
    proc_a_suma -> proc_a_comp;
    proc_a_comp -> proc_a_upd [label="Sí"];
    proc_a_upd -> proc_a_loop;
    proc_a_comp -> proc_a_loop [label="No"];
    proc_a_loop -> salida_a [label="No"];
    
    // Inciso B
    salida_a -> proc_b_loop;
    proc_b_loop -> proc_b_suma [label="Sí"];
    proc_b_suma -> proc_b_comp;
    proc_b_comp -> proc_b_upd [label="Sí"];
    proc_b_upd -> salida_b;
    salida_b -> proc_b_loop;
    proc_b_comp -> proc_b_loop [label="No"];
    proc_b_loop -> proc_c_loop [label="No"];
    
    // Inciso C
    proc_c_loop -> proc_c_comp [label="Sí"];
    proc_c_comp -> salida_c [label="Sí"];
    salida_c -> proc_c_loop;
    proc_c_comp -> proc_c_loop [label="No"];
    proc_c_loop -> fin [label="No"];
}